{
  "name": "gow-teams",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build ",
    "preview": "vite preview",
    "db:create": "npx wrangler d1 create FearlessCards",
    "db:migrations": "npx wrangler d1 migrations apply FearlessCards --local",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate:headers": "npm --prefix ./scripts install && node ./scripts/headers.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-cloudflare": "^4.2.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/typography": "^0.5.12",
    "@types/eslint": "^8.56.7",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-toml": "^2.0.1",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vite": "^5.2.8",
    "wrangler": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "arctic": "^1.5.0",
    "lucia": "^3.1.1",
    "lucide-svelte": "^0.364.0",
    "oslo": "^1.2.0"
  }
}
